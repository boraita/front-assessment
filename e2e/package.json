{
  "name": "front-assessment-e2e",
  "version": "1.0.0",
  "description": "run cypress against anything",
  "scripts": {
    "prestart": "npm install",
    "cypress": "cypress open",
    "cy:local": "cypress run --env env=ci",
    "cypress-debug": "cypress open --config watchForFileChanges=true",
    "cy:verify": "cypress verify",
    "cy:ci": "cypress run --env api_server=http://localhost:3000,env=ci"
  },
  "devDependencies": {
    "cypress": "^9.3.1",
    "eslint-plugin-cypress": "^2.11.1"
  }
}
